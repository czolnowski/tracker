                "Mindweb\\StandardRecognizers\\UserAgentFromHeaderRecognizer",
                "Mindweb\\StandardRecognizers\\ReferrerFromUrlRecognizer",
                "Mindweb\\StandardRecognizers\\ReturningFromUrlRecognizer",
                "Mindweb\\StandardRecognizers\\UUIDRecognizer",
                "Mindweb\\StandardRecognizers\\UTCDateRecognizer"
            ],
            "persistence": [],
            "resolvers": [
                "Mindweb\\StandardResolvers\\JsonResolve"
            ]
        }
    }
}