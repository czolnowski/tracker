{
    "tracker": {
        "debug": "true",
        "db": {
            "type": {
                "vendor": "Mindweb",
                "namespace": "DbMysql",
                "name": "MysqlConnection"
            },
            "adapter": {
                "driver": "pdo_mysql",
                "host": "127.0.0.1",
                "dbname": "analytics",
                "user": "user",
                "password": "password",
                "charset": "utf8"
            }
